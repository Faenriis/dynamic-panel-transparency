#: prefs.js/ui/prefs.ui:346
msgid "Transitions"
msgstr "Transiciones"

#. Allows the user to set their own text color.
#: prefs.js/ui/prefs.ui:403
msgid "Enable custom text coloring"
msgstr "Acticar color de texto personalizado"

#. The main color to be used.
#: prefs.js/ui/prefs.ui:430 prefs.js/ui/prefs.ui:445
msgid "Primary Color"
msgstr "Color primario"

#. Another color for special functions.
#: prefs.js/ui/prefs.ui:459 prefs.js/ui/prefs.ui:473
msgid "Secondary Color"
msgstr "Color secundario"

#: prefs.js/ui/prefs.ui:493
msgid "Use when a window is maximized"
msgstr "Usar cuando una ventana está maximizada"

#: prefs.js/ui/prefs.ui:516
msgid "Use when the overview is visible"
msgstr "Usar cuando la vista general está visible"

#: prefs.js/ui/prefs.ui:584
msgid "Enable text shadowing"
msgstr "Activar sombras de texto"

#: prefs.js/ui/prefs.ui:624 prefs.js/ui/prefs.ui:685 prefs.js/ui/prefs.ui:823
#: prefs.js/ui/prefs.ui:884
msgid "Shadow Color"
msgstr "Color de la sombra"

#: prefs.js/ui/prefs.ui:698 prefs.js/ui/prefs.ui:897
msgid "Radius"
msgstr "Radio"

#: prefs.js/ui/prefs.ui:711 prefs.js/ui/prefs.ui:910
msgid "Horizontal Offset"
msgstr "Desplazamiento horizontal"

#: prefs.js/ui/prefs.ui:724 prefs.js/ui/prefs.ui:923
msgid "Vertical Offset"
msgstr "Desplazamiento vertical"

#: prefs.js/ui/prefs.ui:783
msgid "Enable icon shadowing"
msgstr "Activar sombras de iconos"

#: prefs.js/ui/prefs.ui:969
msgid "Foreground"
msgstr "Primer plano"

#. Opacity of the panel when no window is maximized.
#: prefs.js/ui/prefs.ui:1031
msgid "Unmaximized Opacity"
msgstr "Opacidad sin maximizado"

#. Opacity of the panel when a window is maximized.
#: prefs.js/ui/prefs.ui:1078 prefs.js/ui/app-prefs.ui:117
msgid "Maximized Opacity"
msgstr "Opacidad con maximizado"

#. Allows the user to set their own opacity.
#: prefs.js/ui/prefs.ui:1100
msgid "Enable custom opacity"
msgstr "Activar opacidad personalizada"

#. The panel's background color.
#: prefs.js/ui/prefs.ui:1167 prefs.js/ui/prefs.ui:1182
#: prefs.js/ui/app-prefs.ui:91
msgid "Panel Color"
msgstr "Color del panel"

#. Allows the user to set their own background color for the panel.
#: prefs.js/ui/prefs.ui:1203
msgid "Enable custom panel color"
msgstr "Activar color del panel personalizado"

#. Removes any extra styling certain themes may have. Fixes theme issues.
#: prefs.js/ui/prefs.ui:1267
msgid "Remove excess panel styling"
msgstr "Eliminar estilo sobrante del panel"

#: prefs.js/ui/prefs.ui:1280
msgid "(Fixes theme incompatibilities.)"
msgstr "(Corrigue incompatibilidades del tema.)"

#. Hides the panel corners.
#: prefs.js/ui/prefs.ui:1308
msgid "Hide corners"
msgstr "Ocultar esquinas"

#: prefs.js/ui/prefs.ui:1336
msgid "Background"
msgstr "Fondo"

#: prefs.js/ui/prefs.ui:1438
msgid "About"
msgstr "Acerca de"

#. Adds a 'custom' app tweak. Only visible on the App Tweaks page.
#: prefs.js/ui/prefs.ui:1466
msgid "Advanced..."
msgstr "Avanzado..."

#: prefs.js/ui/prefs.ui:1502
msgid ""
"A WM_CLASS is a code that identifies a window. Use this setting if the "
"window you want custom settings for is not part of an application. You can "
"find a window's WM_CLASS with the command 'xprop WM_CLASS'. Enter the second "
"result from 'xprop WM_CLASS' below without quotation marks."
msgstr ""
"Un WM_CLASS es un código que identifica una ventana. Use este ajuste si la "
"ventana que desea personalizar no es parte de una aplicación. Puede "
"localizar el WM_CLASS de una ventana mediante el comando 'xprop WM_CLASS'. "
"Introduzca el segundo resultado de 'xprop WM_CLASS' sin comillas."

#: prefs.js/ui/prefs.ui:1551
msgid "WM_CLASS cannot be blank."
msgstr "WM_CLASS no puede estar en blanco."

#: prefs.js/ui/app-prefs.ui:51
msgid "Enable background app tweaks"
msgstr "Activar ajustes de la aplicación de fondo"

#: prefs.js/ui/app-prefs.ui:130
msgid "Theme Source"
msgstr "Fuente del tema"

#: prefs.js/ui/app-prefs.ui:190
msgid "Always trigger the panel."
msgstr "Desencadenar siempre el panel"
